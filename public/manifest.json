{
  "manifest_version": 2,
  "name": "Dédé",
  "version": "0.0.1",

  "description": "Pré-remplissage de formulaires",
  "icons": {
    "96": "./icon.png"
  },

  "browser_action": {
    "default_icon": "./icon.png",
    "default_title": "Dédé",
    "default_popup": "./index.html"
  },

  "permissions": ["activeTab"],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
