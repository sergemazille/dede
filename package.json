{
  "name": "dede",
  "version": "0.1.0",
  "description": "Pr√©-remplissage de formulaires",
  "scripts": {
    "start": "npm i && NODE_ENV=production gulp && webpack --config webpack.config.js",
    "build": "NODE_ENV=production gulp && npm run test && npm run webpackBuild && npm run package",
    "package": "cd ./dist && web-ext lint && web-ext build --artifacts-dir ../build",
    "dev": "gulp & jobs && npm run test && webpack --config webpack.config.js -w & jobs && npm run server",
    "dev:serverless": "gulp & jobs && npm run test && webpack --config webpack.config.js -w & jobs",
    "webpackBuild": "NODE_ENV=production webpack --config webpack.config.js",
    "server": "cd ./ancillary/server && composer install && bin/console server:run",
    "test": "npm run lint && jest",
    "lint": "eslint --ext .js,.vue src",
    "create-form": "gulp --color scaffoldNewForm"
  },
  "repository": {
    "type": "git",
